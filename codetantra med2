def check_triangle(a, b, c):
    if a + b > c and b + c > a and c + a > b:
        return True
    else:
        return False

print("Enter the length of three sides of triangle:")
a, b, c = map(int, input().split())

if check_triangle(a, b, c):
    print("Valid triangle")
else:
    print("Invalid triangle")
