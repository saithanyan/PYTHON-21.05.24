def check_rectangle(a, b, c, d):
    sides = sorted([a, b, c, d])
    if sides[0] + sides[1] == sides[2] + sides[3]:
        return True
    else:
        return False
print("Enter rectangle four sides of length:")
a, b, c, d = map(int, input().split())

if check_rectangle(a, b, c, d):
    print("Yes")
else:
    print("No")
